<?xml version="1.0" encoding="UTF-8"?>

    <androidx.constraintlayout.widget.ConstraintLayout
    tools:context=".Registration_admin"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@android:color/white">

    <TextView
        android:layout_height="37dp"
        android:layout_width="324dp"
        app:layout_constraintVertical_bias="0.023"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.183"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textStyle="bold|italic"
        android:textSize="20sp"
        android:textColor="#2E4E5F"
        android:textAllCaps="true"
        android:text="Registration form for admin"
        android:freezesText="false"
        android:id="@+id/title1"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/title1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintEnd_toEndOf="parent"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="#0A0A0A"
        android:textAllCaps="false"
        android:text="Create New Account"
        android:id="@+id/subtitle1"
        android:layout_marginTop="52dp"/>

    <EditText
        android:layout_height="62dp"
        android:layout_width="390dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintEnd_toEndOf="parent"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="@color/colorPrimaryDark"
        android:id="@+id/register_username_admin"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/subtitle1"
        android:textColorHint="@color/colorPrimaryDark"
        android:padding="20dp"
        android:inputType="text"
        android:hint="Full name"> </EditText>

    <EditText
        android:layout_height="62dp"
        android:layout_width="390dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="@color/colorPrimaryDark"
        android:id="@+id/register_phone_number_admin"
        app:layout_constraintTop_toBottomOf="@+id/register_username_admin"
        android:textColorHint="@color/colorPrimaryDark"
        android:padding="20dp"
        android:inputType="number"
        android:hint="Phone number"
        android:layout_below="@+id/register_username_admin"> </EditText>

    <EditText
        android:layout_height="68dp"
        android:layout_width="390dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="@color/colorPrimaryDark"
        android:id="@+id/register_specilazation_admin"
        app:layout_constraintTop_toBottomOf="@+id/register_phone_number_admin"
        android:textColorHint="@color/colorPrimaryDark" android:padding="20dp"
        android:inputType="textEmailAddress"
        android:hint="Email"
        android:layout_alignParentTop="true"> </EditText>

    <EditText
        android:layout_height="64dp"
        android:layout_width="388dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="@color/colorPrimaryDark"
        android:id="@+id/register_workat_admin"
        app:layout_constraintTop_toBottomOf="@+id/register_specilazation_admin"
        android:textColorHint="@color/colorPrimaryDark"
        android:padding="20dp"
        android:inputType="textPassword"
        android:hint="Password"
        android:layout_alignParentTop="true"> </EditText>

    <EditText
        android:layout_height="69dp"
        android:layout_width="390dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintEnd_toEndOf="parent"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="@color/colorPrimaryDark"
        android:id="@+id/register_gender_admin"

        android:textColorHint="@color/colorPrimaryDark"
        android:padding="20dp"
        android:inputType="text"
        android:hint="Address"
        app:layout_constraintBottom_toTopOf="@+id/create_acc_btn1"
        android:layout_marginBottom="12dp"> </EditText>

    <Button
        android:layout_height="wrap_content"
        android:layout_width="178dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.519"
        app:layout_constraintEnd_toEndOf="parent"
        android:textStyle="bold|italic"
        android:textSize="18sp"
        android:textAllCaps="true"
        android:text="Register"
        android:id="@+id/create_acc_btn1"
        android:layout_marginTop="84dp"
        app:layout_constraintTop_toBottomOf="@+id/register_workat_admin"/>

    <TextView
        android:layout_height="32dp"
        android:layout_width="266dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.324"
        app:layout_constraintEnd_toEndOf="parent"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="#0A0A0A"
        android:text="Already register? Login Here"
        android:id="@+id/Login_here1"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/create_acc_btn1"/>

    <ProgressBar
        android:layout_height="25dp"
        android:layout_width="62dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintEnd_toEndOf="parent"
        android:id="@+id/progressBar1"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/Login_here1"
        android:visibility="invisible" style="?android:attr/progressBarStyle"/>

</androidx.constraintlayout.widget.ConstraintLayout>